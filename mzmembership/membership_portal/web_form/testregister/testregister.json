 "client_script": "$( document ).ready(function() {\n    $(\".d-flex.justify-content-between.align-items-center\")[0].children[0].innerHTML = '<img class=\"app-logo\" style=\"width:20%; \"src=\"https://members.mesalumni.org/files/Screenshot%20from%202021-10-08%2015-54-37.png\"><h2>MES Alumni Community Membership Registration</h2>';\n});\n\n\n\n\nfrappe.web_form.on('email', (field, value) => {\n    let data = frappe.web_form.get_value(\"email\");\n\n    if(data){\n        frappe.call({\n            \"method\": \"mesalumniapp.mes_alumni.web_form.mes_alumni_association_member_registration.mes_alumni_association_member_registration.check_user\",\n            \"args\": {\"email\": data},\n            \"callback\": function(r){\n                var data = r.message;\n                if(r.message == true){\n                    frappe.web_form.set_value(\"email\", '');\n                    frappe.throw(\"Already Registered Email\");\n                }\n            }\n        });\n    }\n});\n\nfrappe.web_form.on('qid', (field, value) => {\n    let qid = frappe.web_form.get_value(\"qid\");\n\n    if(qid && (qid.length > 11 || qid.length < 11)){\n        frappe.web_form.set_value(\"qid\", '');\n        frappe.throw(\"QID maximum and minimum limit should be 11 digit\");\n    }\n});\n/*\nfrappe.web_form.on('submit', (field, value) => {\nlet f_name = frappe.web_form.get_value(\"full_name\");\nlet email = frappe.web_form.get_value(\"email\");\nif(f_name && email){\nfrappe.call({\n\"method\": \"mesalumniapp.whitelist.create_user\",\n\"args\": {\"email\": email,\"name\":f_name},\n});\n}\n});\n*/",
